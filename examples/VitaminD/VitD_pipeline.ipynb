    "def save(obj, file):\n",
    "    with open('output/' + file,\"wb\") as file:\n",
    "        pickle.dump(obj,file)\n",
    "\n",
    "def load(file):\n",
    "    with open('output/' + file,\"rb\") as file:\n",
    "        obj = pickle.load(file)\n",
    "\n",
    "    return obj"
   ]
